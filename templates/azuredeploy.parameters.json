{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vmName": {
            "value": "simpleLinuxVM"
        },
        "adminUsername": {
            "value": "azureuser"
        },
        "authenticationType": {
            "value": "sshPublicKey"
        },
        "adminPasswordOrKey": {
            "value": "${{ env.SSH_PUBLIC_KEY }}"
        },
        "dnsLabelPrefix": {
            "value": "myuniqudnslabel-54678"
        },
        "ubuntuOSVersion": {
            "value": "Ubuntu-2204"
        },
        "location": {
            "value": "Central US"
        },
        "vmSize": {
            "value": "Standard_B1s"
        },
        "virtualNetworkName": {
            "value": "vNet-arm"
        },
        "subnetName": {
            "value": "Subnet-arm"
        },
        "networkSecurityGroupName": {
            "value": "SecGroupNet-arm"
        },
        "securityType": {
            "value": "TrustedLaunch"
        },
        "scriptUrl": {
            "value": "https://raw.githubusercontent.com/surya-devops77/git-arm/refs/heads/main/templates/install.sh"
        }
    }
}
