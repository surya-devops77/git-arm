{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vmName": {
            "value": "simpleLinuxVM"
        },
        "adminUsername": {
            "value": "azureuser" 
        },
        "authenticationType": {
            "value": "sshPublicKey"
        },
        "adminPasswordOrKey": {
            "value": "${{ env.SSH_PUBLIC_KEY }}"
        },
        "dnsLabelPrefix": {
            "value": "myuniquednslabel1-546" 
            
        },
        "ubuntuOSVersion": {
            "value": "Ubuntu-2204"
        },
        "location": {
            "value": "Central US" 
        },
        "vmSize": {
            "value": "Standard_B2ms"
        },
        "virtualNetworkName": {
            "value": "vNet-arm"
        },
        "subnetName": {
            "value": "Subnet-arm"
        },
        "networkSecurityGroupName": {
            "value": "SecGroupNet-arm"
        },
        "securityType": {
            "value": "TrustedLaunch"
        },
        "isNewDeployment": {
            "value": false
        }
    }
}

